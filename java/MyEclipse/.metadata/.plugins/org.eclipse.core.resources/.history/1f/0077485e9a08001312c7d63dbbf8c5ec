package PPoker;

public class CPoker 
{
	CCard cards[];
	
	public CPoker()
	{
		int n=0;
		int isuit,jrank;
		this.cards = new CCard[54];
		for(jrank = CCard.THREE ; jrank <= CCard.DEUCE ; jrank++ )
		{
			for(isuit = CCard.HEART; isuit<= CCard.CLUB ; isuit++);
			{
				System.out.println(isuit + " " + jrank);
				this.cards[n++]= new CCard(isuit, jrank);
				System.out.println(this.cards[0].getSuit());
				
			}
		}
		cards[n++].setRank(CCard.JOKER);
		cards[n++].setRank(CCard.COLOR);
	}
}
